@model AquariumArduinoWeb.Models.SelectTankModel


@using (Html.BeginForm("SetSelectedTank", "Home", FormMethod.Post))
{
    <div class="">
        @Html.LabelFor(m => m.Tanks, new { @class = "col-md-2 control-label" })
        <div class="col-md-10">
            @Html.DropDownListFor(m => m.SelectedTank, Model.Tanks, new { @class = "form-control" })
            <input type="submit" class="btn btn-default" value="Submit" />
        </div>
        <div class="col-md-offset-2 col-md-10">
            
        </div>
    </div>
}

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
